  {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "NestJS: Start Debug (3001)",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "start:debug",
          "--",
          "--port",
          "3001"
        ],
        "console": "integratedTerminal",
        "autoAttachChildProcesses": true,
        "port": 9229,
        "restart": true,
        "cwd": "${workspaceFolder}",
        "env": {
          "PORT": "3001"
        },
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"]
      },
      {
        "type": "node",
        "request": "attach",
        "name": "NestJS: Attach (9229)",
        "port": 9229,
        "protocol": "inspector",
        "restart": true,
        "autoAttachChildProcesses": true,
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/dist/**/*.js"]
      }
    ]
  }